<?xml version="1.0"?>
<module>
	<title>Resource: Sitemap</title>
	<description><![CDATA[Support for sitemap generation.]]></description>
	<category>Resource</category>
	<version>0.3.5</version>
	<log version="0.1">Initial prototype.</log>
	<log version="0.2">Separation to Hydrogen module.</log>
	<log version="0.3">Support for compression.</log>
	<log version="0.3.1">Corrected MIME type on plain RSS delivery.</log>
	<log version="0.3.2">Support to submit to providers.</log>
	<log version="0.3.3">Refactoring for delivering MIME type next to content.</log>
	<log version="0.3.4">Improve sitemap information handling.</log>
	<log version="0.3.5">Support HTML view of sitemap with content negotiation.</log>
	<company site="http://ceusmedia.de/">Ceus Media</company>
	<author email="christian.wuerker@ceusmedia.de">Christian W&#xFC;rker</author>
	<files>
		<class>Controller/Sitemap.php5</class>
		<class>Logic/Sitemap.php5</class>
		<class>View/Sitemap.php5</class>
		<locale>de/html/sitemap/bottom.html</locale>
		<locale>de/html/sitemap/top.html</locale>
	</files>
	<config protected="yes" type="string" name="compression" values="bzip,gzip,none">none</config>
	<config protected="yes" type="boolean" name="html.enabled">no</config>
	<link access="public" path="sitemap"></link>
	<link access="public" path="sitemap/index"></link>
	<relations/>
</module>
